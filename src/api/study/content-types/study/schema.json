{
  "kind": "collectionType",
  "collectionName": "studies",
  "info": {
    "singularName": "study",
    "pluralName": "studies",
    "displayName": "Study"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "semester": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 4
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "study"
    }
  }
}
